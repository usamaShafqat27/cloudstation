<%= form_with model: channel, url: channels_path, local: false, html: { class: "my-2" } do |form| %>
  <div class="form-group mb-2 d-flex">
    <%= form.text_field :name, placeholder: "Create New Channel", class: "form-control me-2 dark-form-input" %>
    <%= form.submit 'Create', class: "btn btn-primary primary-btn" %>
  </div>

  <div class="actions">
  </div>
<% end %>